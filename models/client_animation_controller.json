{
	"$schema": "../models_schema/root.json",
	"pack_type": "resource-pack",
	"type": "JSON",
	"multi": true,
	"path": ["RP/animation_controllers/**/*.json"],
	"json_path": ["animation_controllers", "@STR"],
	"identifier": {
		"json-path-key": {
			"path_start": "object",
			"path": [],
			"processing": {
				"filters": {
					"exclude": false,
					"items": ["test", {"starts_with": "animation.controller."}]
				}
			}
		}
	},
	"properties": {
		"state": {
			"custom_value": {
				"expected_types": ["object"],
				"access_paths": [
					{
						"json_path_key": {
							"multi": true,
							"path_start": "object",
							"path": ["animation_controllers", "@STR", "states"],
							"subproperties": {
								"animation": {
									"alias_reference": {
										"map_provider": "client_entity",
										"map_path": ["animations"],
										"access_paths": [
											{
												"json_path_value": {
													"multi": true,
													"path_start": "object",
													"path": ["animations", "@INT"]
												}
											},
											{
												"json_path_key": {
													"multi": true,
													"path_start": "object",
													"path": ["animations", "@INT", "@STR"]
												}
											}
										]
									}
								},
								"particle_effect": {
									"alias_reference": {
										"map_provider": "client_entity",
										"map_path": ["particle_effects"],
										"access_paths": [
											{
												"json_path_value": {
													"multi": true,
													"path_start": "object",
													"path": ["particle_effects", "@INT", "effect"]
												}
											}
										]
									}
								},
								"sound_effect": {
									"alias_reference": {
										"map_provider": "client_entity",
										"map_path": ["sound_effects"],
										"access_paths": [
											{
												"json_path_value": {
													"multi": true,
													"path_start": "object",
													"path": ["sound_effects", "@INT", "effect"]
												}
											}
										]
									}
								}
							}
						}
					}
				]
			}
		}
	}
}